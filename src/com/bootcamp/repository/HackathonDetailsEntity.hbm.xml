<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class schema="bootcamp" table="hackathon_details" name="com.bootcamp.model.HackathonDetailsEntity">
    <id name="id" column="id"/>
    <property name="eventName" column="Event_name"/>
    <property name="moOffice" column="moOffice"/>
    <property name="dateConducted" column="DateConducted"/>
    <property name="totalIdea" column="totalIdea"/>
    </class>

</hibernate-mapping>